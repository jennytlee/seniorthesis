@phdthesis{bradley_evaluating_2017,
	address = {US},
	title = {Evaluating the effects of mastery learning in postsecondary developmental mathematics},
	abstract = {The purpose of this study was to determine which academic and affective student factors were significant to student success in a mastery learning program in developmental mathematics and to determine if the mastery learning program led to increased mathematical knowledge retention and success in the subsequent math course. The first phase of the study involved 298 students in developmental mathematics. Using binary logistic regression, the researcher found that placement test score, previous experience with mastery learning, and gender were statistically significant factors related to student success in the mastery learning program. The second phase of the study involved 529 students in the subsequent course intermediate algebra. Using one-way analysis of variance and chi-square methods, the researcher found that mastery learning students retained significantly more mathematical content knowledge and succeeded at significantly higher rates in the subsequent course intermediate algebra as compared to students who took developmental mathematics through traditional methods and those who opted out of placement testing. Results of the study showed that the mastery learning approach in developmental mathematics which incorporated the affective student domain offered advantages for assisting students to overcome math background and self-efficacy barriers. (PsycINFO Database Record (c) 2017 APA, all rights reserved)},
	school = {ProQuest Information \& Learning},
	author = {Bradley, Kirk},
	year = {2017},
	keywords = {2017, course evaluation, Course Evaluation, developmental mathematics, mastery learning, Mastery Learning, Mathematics, self-efficacy, Self-Efficacy},
	annote = {The study looks at community college students in intermediate algebra under a mastery learning environment. Results showed fewer dropouts and failed grades, as well as higher test scores for students that underwent the modified classes.},
	file = {out.pdf:/Users/jennylee/Zotero/storage/VMGSNPDT/out.pdf:application/pdf}
}

