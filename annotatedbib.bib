	month = jan,
	year = {2011},
	keywords = {Qualitative Research, Remedial Mathematics, Student Attitudes, College Mathematics, Teaching Methods, Questionnaires, Student Motivation, Accuracy, Mathematical Concepts, Problem Solving, United States}
}

